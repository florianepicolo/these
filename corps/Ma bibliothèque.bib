@article{escriva_ligand_1997,
	title = {Ligand binding was acquired during evolution of nuclear receptors},
	volume = {94},
	issn = {0027-8424},
	doi = {10.1073/pnas.94.13.6803},
	abstract = {The nuclear receptor (NR) superfamily comprises, in addition to ligand-activated transcription factors, members for which no ligand has been identified to date. We demonstrate that orphan receptors are randomly distributed in the evolutionary tree and that there is no relationship between the position of a given liganded receptor in the tree and the chemical nature of its ligand. NRs are specific to metazoans, as revealed by a screen of NR-related sequences in early- and non-metazoan organisms. The analysis of the NR gene duplication pattern during the evolution of metazoans shows that the present NR diversity arose from two waves of gene duplications. Strikingly, our results suggest that the ancestral NR was an orphan receptor that acquired ligand-binding ability during subsequent evolution.},
