	file = {Xu et al. - 2019 - The allotetraploid origin and asymmetrical genome .pdf:/home/fpicolo/Zotero/storage/X9FJ344E/Xu et al. - 2019 - The allotetraploid origin and asymmetrical genome .pdf:application/pdf},
