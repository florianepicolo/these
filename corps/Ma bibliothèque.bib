@article{vettermann_allelic_2010,
	title = {Allelic exclusion of immunoglobulin genes: models and mechanisms},
	volume = {237},
	issn = {1600-065X},
	shorttitle = {Allelic exclusion of immunoglobulin genes},
	doi = {10.1111/j.1600-065X.2010.00935.x},
	abstract = {The allelic exclusion of immunoglobulin (Ig) genes is one of the most evolutionarily conserved features of the adaptive immune system and underlies the monospecificity of B cells. While much has been learned about how Ig allelic exclusion is established during B-cell development, the relevance of monospecificity to B-cell function remains enigmatic. Here, we review the theoretical models that have been proposed to explain the establishment of Ig allelic exclusion and focus on the molecular mechanisms utilized by developing B cells to ensure the monoallelic expression of Ig kappa and Ig lambda light chain genes. We also discuss the physiological consequences of Ig allelic exclusion and speculate on the importance of monospecificity of B cells for immune recognition.},
	language = {eng},
	number = {1},
