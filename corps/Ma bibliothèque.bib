	keywords = {evolution, Evolution, Molecular, Phylogeny, Biological Evolution, Bacteria, Proteins, Databases, Protein, adaptation, Adaptation, Biological, Archaea, Eukaryota, homeostasis, Homeostasis, membrane proteins, Membrane Proteins, orthologs, Prokaryotic Cells, Sequence Analysis, Protein, Solubility, Water},
	pages = {2874--2884},
