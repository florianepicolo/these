	keywords = {Animals, Humans, Ligands, Cell Communication, Cancer, Receptor, Insulin, Somatomedins, Growth, Insulin-like growth factor binding proteins, Insulin-Like Growth Factor Binding Proteins, Insulin-like growth factor receptors, Insulin-like growth factors, Metabolism},
	pages = {101245},
