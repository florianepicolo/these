	pages = {R876--887},
	file = {Texte intégral:/home/fpicolo/Zotero/storage/46LZUTN3/Telford et al. - 2015 - Phylogenomic Insights into Animal Evolution.pdf:application/pdf},
}

@article{kachroo_evolution_2015,
	title = {Evolution. {Systematic} humanization of yeast genes reveals conserved functions and genetic modularity},
	volume = {348},
	issn = {1095-9203},
	doi = {10.1126/science.aaa0769},
	abstract = {To determine whether genes retain ancestral functions over a billion years of evolution and to identify principles of deep evolutionary divergence, we replaced 414 essential yeast genes with their human orthologs, assaying for complementation of lethal growth defects upon loss of the yeast genes. Nearly half (47\%) of the yeast genes could be successfully humanized. Sequence similarity and expression only partly predicted replaceability. Instead, replaceability depended strongly on gene modules: Genes in the same process tended to be similarly replaceable (e.g., sterol biosynthesis) or not (e.g., DNA replication initiation). Simulations confirmed that selection for specific function can maintain replaceability despite extensive sequence divergence. Critical ancestral functions of many essential genes are thus retained in a pathway-specific manner, resilient to drift in sequences, splicing, and protein interfaces.},
	language = {eng},
