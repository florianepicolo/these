	keywords = {Apoptosis, Signal Transduction, Humans, Cell Movement, Extracellular Signal-Regulated MAP Kinases, Cell Differentiation, Mitogen-Activated Protein Kinases, Cell Proliferation, Cellular Senescence, Extracellular signal-regulated kinase, mitogen-activated protein kinases, signaling pathway},
	pages = {600--604},
}

