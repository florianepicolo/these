	year = {2010},
	pmid = {21618822},
	keywords = {Animals, Humans, Phylogeny, Protein Structure, Tertiary, Biological Evolution, Receptors, G-Protein-Coupled, Protein Isoforms},
	pages = {1--13},
}

@article{gabaldon_patterns_2020,
