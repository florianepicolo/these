	pmid = {33461212},
	pmcid = {PMC7875771},
	pages = {284--289},
	file = {Texte intégral:/home/fpicolo/Zotero/storage/RBDEC4TN/Meyer et al. - 2021 - Giant lungfish genome elucidates the conquest of l.pdf:application/pdf},
