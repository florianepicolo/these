	keywords = {Signal Transduction, Cyclic AMP, Plant Diseases, Adenosine Monophosphate, Ca2+ signaling, Calcium Chloride, cAMP signaling, Capsicum, Colletotrichum scovillei, Fruit, fruit anthracnose, pepper, Pharmaceutical Preparations},
	pages = {1003195},
	file = {Texte intégral:/home/fpicolo/Zotero/storage/WWKMCDWG/Fu et al. - 2022 - Role of the cAMP signaling pathway in the dissemin.pdf:application/pdf},
}

