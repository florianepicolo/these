	pmcid = {PMC6651758},
	keywords = {evolution, Animals, Signal Transduction, Humans, Models, Biological, receptor, cell signaling, Cells, crosstalk, ligand, Protein Processing, Post-Translational, signal transduction, Translational Research, Biomedical},
	pages = {3292},
	file = {Texte intégral:/home/fpicolo/Zotero/storage/S2DGPUB3/Nair et al. - 2019 - Conceptual Evolution of Cell Signaling.pdf:application/pdf},
}

