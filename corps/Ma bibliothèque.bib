	keywords = {Animals, Evolution, Molecular, Genes, Humans, Biological Evolution, Eukaryota, Genomics, Phenotype, Male, Testis},
	pages = {1313--1326},
	file = {Texte intégral:/home/fpicolo/Zotero/storage/UUWKSBGR/Kaessmann - 2010 - Origins, evolution, and phenotypic impact of new g.pdf:application/pdf},
}

