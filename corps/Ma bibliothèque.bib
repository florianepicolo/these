@article{guo_hox_2010,
	title = {Hox genes of the {Japanese} eel {Anguilla} japonica and {Hox} cluster evolution in teleosts},
	volume = {314},
	issn = {1552-5015},
	doi = {10.1002/jez.b.21318},
	abstract = {Compared with other diploid teleosts (2n=48), anguilloid fish have a specialized karyotype (2n=38) and remarkable morphological variation, and represent one basal group species of teleosts. To investigate the Hox gene/cluster inventory in basal teleosts, a PCR-based survey of Hox genes in the Japanese eel (Anguilla japonica) was conducted with both gene-specific and homeobox-targeted degenerate primers. Our data provide evidence that at least 34 distinct Hox genes exist in the Japanese eel genome and that they represent eight Hox clusters. Duplication of Hox genes in the Japanese eel appears to be the result of the fish-specific genome duplication (FSGD) event. The Japanese eel shared the FSGD event with other teleosts such as zebrafish and pufferfish. A member of Hox paralog group one (HoxA1b) was preserved in the Japanese eel but was lost in other teleosts. Available Hox data revealed that the Hox cluster evolved distinctly in different teleost lineages. All duplicated Hox clusters were retained after the FSGD event in basal teleosts like in the Japanese eel, whereas crown teleosts lost one cluster (HoxCb or HoxDb). Based on current teleostean phylogeny, the HoxDb cluster was lost independently in the teleost lineages Otocephala and Euteleostei.},
	language = {eng},
	number = {2},
	journal = {J Exp Zool B Mol Dev Evol},
	author = {Guo, Baocheng and Gan, Xiaoni and He, Shunping},
	month = mar,
	year = {2010},
	pmid = {19670462},
	keywords = {Animals, Evolution, Molecular, Genomics, Female, DNA Primers, Polymerase Chain Reaction, Genes, Homeobox, Homeodomain Proteins, Anguilla},
	pages = {135--147},
}

@article{amores_zebrafish_1998,
	title = {Zebrafish hox clusters and vertebrate genome evolution},
	volume = {282},
	issn = {0036-8075},
	doi = {10.1126/science.282.5394.1711},
	abstract = {HOX genes specify cell fate in the anterior-posterior axis of animal embryos. Invertebrate chordates have one HOX cluster, but mammals have four, suggesting that cluster duplication facilitated the evolution of vertebrate body plans. This report shows that zebrafish have seven hox clusters. Phylogenetic analysis and genetic mapping suggest a chromosome doubling event, probably by whole genome duplication, after the divergence of ray-finned and lobe-finned fishes but before the teleost radiation. Thus, teleosts, the most species-rich group of vertebrates, appear to have more copies of these developmental regulatory genes than do mammals, despite less complexity in the anterior-posterior axis.},
