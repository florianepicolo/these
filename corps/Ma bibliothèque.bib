	journal = {Commun Biol},
	author = {Kon, Tetsuo and Fukuta, Kentaro and Chen, Zelin and Kon-Nanjo, Koto and Suzuki, Kota and Ishikawa, Masakazu and Tanaka, Hikari and Burgess, Shawn M. and Noguchi, Hideki and Toyoda, Atsushi and Omori, Yoshihiro},
