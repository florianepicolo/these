	author = {Livera, Gabriel and Uzbekov, Rustem and Jarrier, Peggy and Fouchécourt, Sophie and Duquenne, Clotilde and Parent, Anne-Simone and Marine, Jean-Christophe and Monget, Philippe},
	year = {2016},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/1873-3468.12275},
	keywords = {female fertility, Mdm2, oocyte, ovary, p53},
	pages = {2566--2574},
