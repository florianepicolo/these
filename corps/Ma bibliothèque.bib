	author = {Chartier-Harlin, Marie-Christine and Kachergus, Jennifer and Roumier, Christophe and Mouroux, Vincent and Douay, Xavier and Lincoln, Sarah and Levecque, Clotilde and Larvor, Lydie and Andrieux, Joris and Hulihan, Mary and Waucquier, Nawal and Defebvre, Luc and Amouyel, Philippe and Farrer, Matthew and Destée, Alain},
