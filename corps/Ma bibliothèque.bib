	keywords = {Animals, Evolution, Molecular, Molecular Sequence Data, Base Sequence, Neuropeptides, Receptors, G-Protein-Coupled, Chordata, Nonvertebrate, Petromyzon, Placozoa, Platynereis, proenkephalin},
	pages = {8702--8707},
	file = {Texte intégral:/home/fpicolo/Zotero/storage/XKKC9TAE/Jékely - 2013 - Global view of the evolution and diversity of meta.pdf:application/pdf},
